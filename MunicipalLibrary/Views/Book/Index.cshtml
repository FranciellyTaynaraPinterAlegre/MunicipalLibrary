@model IEnumerable<MunicipalLibrary.Models.Book>

@{
    ViewBag.Title = "Lista de livros";
}

<p>
    @Html.ActionLink("New Book", "New", null, new { @class = "btn btn-primary" })
</p>

if(@Model.Count() == 0 ){
    <h2>Página não existente!</h2>
}else{
<table id="books" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Book</th>
            <th>Genre</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
}


}


